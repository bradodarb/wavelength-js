{
  "name": "wavelength-js",
  "version": "3.0.16",
  "license": "MIT",
  "author": "Brad Murry",
  "description": "FAAS Micro Framework",
  "homepage": "https://bradodarb.github.com/wavelength-js/README.md",
  "keywords": [
    "TypeScript",
    "JavaScript",
    "FAAS",
    "aws",
    "lambda",
    "middleware"
  ],
  "libraryFile": "index",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradodarb/wavelength-js.git"
  },
  "bugs": {
    "url": "https://github.com/bradodarb/wavelength-js/issues"
  },
  "scripts": {
    "build": "rollup -c rollup.config.ts",
    "watch": "rollup -cw rollup.config.ts",
    "configure-npm": "node ./set-registry.js -s https://registry.npmjs.org",
    "configure-gpr": "node ./set-registry.js -s https://npm.pkg.github.com",
    "lint": "eslint src/** --fix",
    "test": "jest test/unit/**",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage test/unit"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/aws-sdk": "^2.7.0",
    "@types/bunyan": "^1.8.6",
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.160",
    "@types/node": "^14.6.0",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/source-map-support": "^0.5.3",
    "aws-event-mocks": "^0.0.0",
    "aws-lambda-mock-context": "^3.2.1",
    "jest": "^26.2.2",
    "jest-config": "^26.2.2",
    "rollup": "^2.23.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-module-replacement": "^1.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-uglify": "^6.0.4",
    "source-map-support": "^0.5.19",
    "terser": "^5.0.0",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "aws-sdk": "^2.740.0",
    "bunyan": "^1.8.14",
    "fast-safe-stringify": "^2.0.7",
    "lodash": "^4.17.20"
  }
