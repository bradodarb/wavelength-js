{
  "name": "wavelength-js",
  "version": "1.0.0",
  "description": "Serverless micro-framework and support utilities",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "eslint src/**",
    "test": "jest test/unit/**",
    "build": "babel source --presets babel-preset-es2015 --out-dir distribution",
    "coverage": "jest --collectCoverageFrom=src/**.js --coverage test/unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradodarb/wavelength-js.git"
  },
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "nodejs",
    "javascript",
    "es6"
  ],
  "author": "Brad Murry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradodarb/wavelength-js/issues"
  },
  "homepage": "https://github.com/bradodarb/wavelength-js#readme",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "@types/jest": "^21.X",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "^6.0.0",
    "eslint": "5.0.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "install": "^0.12.2",
    "jest": "23.1.0",
    "jest-ajv": "1.0.3",
    "jest-cli": "^21.X",
    "jest-html-reporter": "^2.4.2",
    "jest-xml-matcher": "^1.1.1",
    "mockdate": "^2.0.2",
    "npm": "^6.4.1"
  },
  "dependencies": {
    "aws-sdk": "^2.244.1",
    "axios": "^0.18.0",
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.2",
    "fast-safe-stringify": "^2.0.6",
    "hoek": "^6.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "reusify": "^1.0.4",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-jest-plugin": "^0.1.6",
    "uuid": "^3.0.1",
    "mem": "4.0.0"
  },
  "jest": {
    "clearMocks": true,
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    }
  }
}
